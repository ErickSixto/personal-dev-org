<?xml version="1.0" encoding="UTF-8" ?>
<LightningComponentBundle xmlns="http://soap.sforce.com/2006/04/metadata">
    <apiVersion>57.0</apiVersion>
    <isExposed>true</isExposed>
    <targets>
        <target>lightningCommunity__Page</target>
        <target>lightningCommunity__Default</target>
    </targets>
    <targetConfigs>
        <targetConfig targets="lightningCommunity__Default">
                    <property
        name="flowApiName"
        type="String"
        default="Browse_Products"
        label="Flow API Name"
        description="API Name of the Flow to be executed"
      />
            <property
        name="basketId"
        type="String"
        label="Basket ID"
        description="Basket ID for the flow"
      />
            <property
        name="bookingDate"
        type="String"
        label="Booking Date"
        description="Booking Date for the flow. The expected format is 'yyyy-MM-dd'."
      />
            <property
        name="destinationId"
        type="String"
        label="Destination ID"
        description="Destination ID for the flow"
      />
            <property
        name="experienceId"
        type="String"
        label="Experience ID"
        description="Experience ID for the flow"
      />
            <property
        name="repBasketId"
        type="String"
        label="Rep Basket ID"
        description="Rep Basket ID for the flow"
      />
        </targetConfig>
    </targetConfigs>
</LightningComponentBundle>
