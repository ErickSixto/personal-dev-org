    <formulas>
        <name>ChargeHeader</name>
        <dataType>String</dataType>
        <expression>&apos;&lt;p style=&quot;text-align: left; font-size: 16px&quot; align=&quot;left&quot;&gt;&lt;b&gt;- &apos;+IF(ISPICKVAL(  {!Get_Contact.Idioma_de_Preferencia__c},&apos;Español&apos;), &apos;Cuota &apos;, &apos;Fee &apos;)  +TEXT( {!CurrentFeeCounter}) +&apos; - &lt;/b&gt;&lt;/p&gt;&apos;</expression>
    </formulas>
    <formulas>
        <name>FeeCharge</name>
