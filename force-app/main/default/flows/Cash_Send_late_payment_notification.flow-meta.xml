    </formulas>
    <formulas>
        <name>PaymentDate</name>
        <dataType>String</dataType>
        <expression>&apos;&lt;p style=&quot;text-align: left; font-size: 16px&quot; align=&quot;left&quot;&gt;&apos;+ 
IF(ISPICKVAL({!Get_Contact.Idioma_de_Preferencia__c},&apos;Español&apos;), 
  &apos;Fecha de pago: &apos; +
  TEXT(DAY({!Loop_cash_records.pba_financial__Date__c})) + &apos;/&apos; + 
  TEXT(MONTH({!Loop_cash_records.pba_financial__Date__c})) + &apos;/&apos; + 
  TEXT(YEAR({!Loop_cash_records.pba_financial__Date__c})), 
  &apos;Payment date: &apos; + 
  TEXT(MONTH({!Loop_cash_records.pba_financial__Date__c})) + &apos;/&apos; +
  TEXT(DAY({!Loop_cash_records.pba_financial__Date__c})) + &apos;/&apos; + 
  TEXT(YEAR({!Loop_cash_records.pba_financial__Date__c}))
) +
&apos;&lt;/p&gt;&apos;</expression>
